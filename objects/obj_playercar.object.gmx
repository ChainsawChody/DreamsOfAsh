            <string>///Driving / Exit car / Skip intro
var cargo = true;

//Move car
if(x &lt;= 2380)
{
    phy_position_x += 1.5;
    image_speed = .15;
 
    if(x &lt;= 2250)
    {
        phy_position_x += 2;
        image_speed = .2;
       
        if(x &lt;= 2000)
        {
            phy_position_x += 3; //3
            image_speed = .5;
        }
    }
}
else
{   
    //Have player wait to exit
    playersit -= 1;
    if(playersit == 0)
    {
        image_speed = 0;
        sprite_index = spr_vehicledead;
        cargo = false;
        // &gt; Play car door sound here
    }
}

//Player gets out of car
if(cargo == false)
{
    instance_create(x, y - 16, obj_cPlayer)
    cargo = true;
}

//Skip intro
if(obj_input.pause_key or obj_input.attack_key)
{
    room_goto(rm_bridge)
}

// play music
if(instance_exists(obj_playercar))
{
    if(!audio_is_playing(snd_forestintro))
    {
        audio_play_sound(snd_forestintro, 1, false)
    }
    if(instance_exists(obj_cPlayer))
    {
        if(obj_cPlayer.x &lt;= 2544)
